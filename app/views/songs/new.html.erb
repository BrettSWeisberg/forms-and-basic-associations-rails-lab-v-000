<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.text_field :collection_select :genre_id, Genre.all, :genre_id, :genre_id
  <%= f.text_field :genre_id %>

  <!--  <%=label :song_notes_1 %>
    <%=text_field :song_notes_1 %>
    <%=label :song_notes_2 %>
    <%=text_field :song_notes_2 %> -->

  <br />

  <%= f.submit %> -->
<% end %>
